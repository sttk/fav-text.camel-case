{
  "name": "@fav/text.camel-case",
  "version": "1.0.3",
  "description": "Converts a string into camel case.",
  "main": "index.js",
  "files": [
    "index.js",
    "index.js.map",
    "index.d.ts",
    "web/"
  ],
  "scripts": {
    "lint": "eslint src/** --ext .ts --fix",
    "test": "mocha --require source-map-support/register test",
    "coverage": "nyc --exclude-after-remap=false --reporter=lcov --reporter=text-summary npm test",
    "coveralls": "nyc --exclude-after-remap=false --reporter=text-lcov npm test | coveralls",
    "web:build": "browserify index.js --standalone fav.text.camel-case -o web/fav.text.camel-case.js && cd web && uglifyjs fav.text.camel-case.js --compress --mangle -o fav.text.camel-case.min.js --source-map url=\"'fav.text.camel-case.min.js.map'\" && cd .. && node tool/make-web-test.js",
    "web:test": "node tool/open-web-test.js",
    "chrome:install": "npm i --no-save mocha-chrome",
    "chrome:test": "mocha-chrome test/web/browser-test.html",
    "build": "npm run lint && tsc && npm run coverage && npm run web:build",
    "test:typescript": "tsc --esModuleInterop test/ts/camel-case.test.ts && node test/ts/camel-case.test.js",
    "test:esmodule": "node --experimental-modules test/mjs/camel-case.test.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sttk/fav-text.camel-case.git"
  },
  "keywords": [
    "camel",
    "case",
    "string",
    "text",
    "fav"
  ],
  "author": "Takayuki Sato",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sttk/fav-text.camel-case/issues"
  },
  "homepage": "https://github.com/sttk/fav-text.camel-case#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "browserify": "^16.5.0",
    "chai": "^3.5.0",
    "coveralls": "^3.0.6",
    "eslint": "^6.2.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "mocha": "^3.5.3",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "source-map-support": "^0.5.13",
    "typescript": "^3.5.3",
    "uglify-js": "^3.6.0"
  }
}
